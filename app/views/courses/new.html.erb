<%= form_for @course do |f| %>
    <%= f.select :year, Course.years.keys.to_a %>
    <%= f.select :semester, Course.semesters.keys.to_a %>
    <%= f.select :date, Course.dates.keys.to_a %>
    <%= f.select :period, Course.periods.keys.to_a %>
    <%= f.text_field :lecture %>
    <%= f.submit "登録する" %>
<% end %>